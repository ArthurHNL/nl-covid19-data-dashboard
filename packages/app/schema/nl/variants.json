{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "nl_variants",
  "required": ["values"],
  "additionalProperties": false,
  "properties": {
    "values": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/variant"
      }
    }
  },
  "definitions": {
    "variant": {
      "type": "object",
      "title": "nl_variants_variant",
      "additionalProperties": false,
      "required": ["name", "values", "last_value"],
      "properties": {
        "name": {
          "type": "string"
        },
        "values": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/value"
          }
        },
        "last_value": {
          "$ref": "#/definitions/value"
        }
      }
    },
    "value": {
      "type": "object",
      "title": "nl_variants_variant_value",
      "additionalProperties": false,
      "required": [
        "occurrence",
        "is_variant_of_concern_or_interest",
        "is_variant_of_concern_or_interest_percentage",
        "is_historical_significance",
        "historical_significance_percentage",
        "sample_size",
        "date_start_unix",
        "date_end_unix",
        "date_of_insertion_unix"
      ],
      "properties": {
        "occurrence": {
          "type": "integer"
        },
        "is_variant_of_concern_or_interest": {
          "type": "boolean"
        },
        "is_variant_of_concern_or_interest_percentage": {
          "type": "number"
        },
        "is_historical_significance": {
          "type": "boolean"
        },
        "historical_significance_percentage": {
          "type": "number"
        },
        "sample_size": {
          "type": "integer"
        },
        "date_start_unix": {
          "type": "integer"
        },
        "date_end_unix": {
          "type": "integer"
        },
        "date_of_insertion_unix": {
          "type": "integer"
        }
      }
    }
  }
}
