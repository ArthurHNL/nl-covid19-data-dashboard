{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "vr_collection_sewer",
  "additionalProperties": false,
  "required": [
    "date_unix",
    "vrcode",
    "average",
    "date_of_insertion_unix"
  ],
  "properties": {
    "date_unix": {
      "type": "integer"
    },
    "vrcode": {
      "type": "string",
      "pattern": "^VR[0-9]+$"
    },
    "average": {
      "type": "number"
    },
    "date_of_insertion_unix": {
      "type": "integer"
    }
  }
}
